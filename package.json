{
  "name": "sux-ds",
  "version": "0.0.1",
  "description": "Substrate Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/substrate-ds/substrate-ds.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --docs --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "build-storybook": "build-storybook -s ./dist -o ./sb-static",
    "start-dev": "npm run build && npm run start && npm run storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@pxtrn/storybook-addon-docs-stencil": "^1.0.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-storysource": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@storybook/web-components": "^6.3.12",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^2.0.1",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
